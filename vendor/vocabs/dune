(rule
 (targets activitystreams2.xml rdfs.xml owl.xml)
 (deps Makefile activitystreams2.ttl rdfs.ttl owl.ttl)
 (action
  (run make %{targets})))

(install
 (section share)
 (package geopub)
 (files
  (activitystreams2.xml as vocabs/activitystreams2.xml)
  (musicontology.xml as vocabs/musicontology.xml)
  (geo.xml as vocabs/geo.xml)
  (rdfs.xml as vocabs/rdfs.xml)
  (owl.xml as vocabs/owl.xml)))
