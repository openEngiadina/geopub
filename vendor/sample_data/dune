(rule
 (targets hello.nt)
 (deps hello.ttl)
 (action
  (with-stdout-to
   hello.nt
   (run rapper -i turtle -o ntriples %{deps}))))

(install
 (section share)
 (package geopub)
 (files
  (hello.nt as sample_data/hello.nt)
  (../../tools/osm_extractor/che.osm.surveillance.nt
   as
   sample_data/che.osm.surveillance.nt)))
